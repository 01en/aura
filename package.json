{
  "name": "gdg-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"vue-cli-service build --mode development --watch\" \"firebase serve --port 8080 -o '0.0.0.0' --token ${FIREBASE_TOKEN}\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "detect-browser": "^4.8.0",
    "material-design-icons-iconfont": "^3.0.3",
    "register-service-worker": "^1.5.2",
    "roboto-fontface": "*",
    "vue": "^2.6.6",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.0.1",
    "compression-webpack-plugin": "^6.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-tools": "^7.6.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
